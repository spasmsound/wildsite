{% extends 'base.html.twig' %}

{% block body %}

    {% for video in videos %}
        <div style="position:relative;height: 0; padding-bottom:29.81%; ">
            <iframe src="https://www.youtube.com/embed/{{ video.getVideoId }}?ecver=2"
                    style="position: absolute;width:50%;height:100%;left:25%" width="1208" height="360" frameborder="0"
                    allow="autoplay; encrypted-media" allowfullscreen></iframe>
        </div>
        <br>

    {% endfor %}
{% endblock %}
